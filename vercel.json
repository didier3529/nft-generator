{
  "version": 2,
  "buildCommand": "cd client && node setup-for-vercel.js && npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 